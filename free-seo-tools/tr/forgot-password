<script type="text/javascript"> 
            jQuery(document).ready(function ($) {
                $("#backtotop").hide(); 
                $(window).scroll(function () { 
                    if ($(this).scrollTop() > 500) { 
                        $('#backtotop').fadeIn(); 
                    } else { 
                        $('#backtotop').fadeOut(); 
                    } 
                });   
            });

            jQuery('.backtotop').click(function () { 
                jQuery('html, body').animate({ 
                    scrollTop: 0 
                }, 'slow'); 
            });
        </script>

